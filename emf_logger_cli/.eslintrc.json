{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "prettier/prettier": "warn",
        "arrow-body-style": "off",
        "prefer-arrow-callback": "off",
        "no-restricted-syntax": "warn",
        "no-plusplus": "off",
        "class-methods-use-this": "off"
    }
}
